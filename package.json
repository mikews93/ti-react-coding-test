    "@testing-library/react": "^11.2.6",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "axios": "^0.21.1",
    "chai": "^4.2.0",
    "classnames": "^2.2.6",
    "json-server": "^0.17.1",
    "lodash": "^4.17.21",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-md": "^1.12.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "4.0.3",
    "react-toastify": "^7.0.4",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start-website": "PORT=4000 react-scripts start",
    "start": "npm-run-all --parallel start-website start-server",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-server": "cd api && npm i && npm start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/store/(index|combineReducers|Context).js",
      "!<rootDir>/src/(index|testConfig|entityNames|constants|serviceWorker).js",
      "!<rootDir>/src/scenes/*?/index.js",
      "!<rootDir>/src/utils/**/index.js",
      "!<rootDir>/src/scenes/*/**/index.js",
      "!<rootDir>/src/components/index.js",
      "!<rootDir>/src/components/*/**/index.js",
      "!<rootDir>/src/constants.js"
    ]
  },
  "devDependencies": {
    "@types/webfontloader": "^1.6.32"
  }
}
