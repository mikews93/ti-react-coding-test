{
  "name": "ti-react-coding-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "classnames": "^2.2.6",
    "enzyme-adapter-react-16": "^1.14.0",
    "evercheck-utils": "^0.1.3",
    "json-server": "^0.15.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-md": "^1.12.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "4.0.3",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start-website": "PORT=4000 react-scripts start",
    "start": "npm-run-all --parallel start-website start-server",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-server": "json-server --watch api/db.json -d 500"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/store/(index|combineReducers|Context).js",
      "!<rootDir>/src/(index|testConfig|entityNames|constants|serviceWorker).js",
      "!<rootDir>/src/scenes/*?/index.js",
      "!<rootDir>/src/utils/**/index.js",
      "!<rootDir>/src/provider/*",
      "!<rootDir>/src/scenes/*/**/index.js",
      "!<rootDir>/src/components/index.js",
      "!<rootDir>/src/components/*/**/index.js",
      "!<rootDir>/src/constants.js"
    ]
  }
}
